
.bottom-tab {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 110rpx;
  box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, 0.5);
  background-color: #ffffff;
}

.fix {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  width: 50%;
}

.user {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  width: 50%;
}

.ifx-images {
  width: 48rpx;
  height: 48rpx;
}

.user-images {
  width: 48rpx;
  height: 48rpx;
  font-family: dls-icons-24;
  font-size: 48rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  color: #cfcfcf
}

.fix-text {
  width: 96rpx;
  height: 34rpx;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: 500;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #1a3670;
}

.user-text {
  width: 96rpx;
  height: 34rpx;
  font-family: PingFangSC;
  font-size: 24rpx;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #cfcfcf;
}
